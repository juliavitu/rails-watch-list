<h1>Bookmarks#new</h1>
<p>Find me in app/views/bookmarks/new.html.erb</p>
<%= simple_form_for @list.bookmark do |f| %>
      <%= f.input :title, required: false %>
      <%= f.input :overview %>
      <%= f.input :poster_url %>
      <%= f.input :rating %>
      <%= f.submit "Create a new bookmark" %>
    <% end %>
